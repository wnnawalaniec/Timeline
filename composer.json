{
    "name": "wojciech.nawalaniec/timeline",
    "description": "Simple lib for managing and organizing events in time.",
    "type": "library",
    "require": {
        "php": "^7.1|^7.3|^7.4|^8.0"
    },
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5.6",
        "infection/infection": "^0.23.0",
        "phpro/grumphp": "^v1.0.0",
        "squizlabs/php_codesniffer": "^3.6.0",
        "phpstan/phpstan": "^0.12.91"
    },
    "authors": [
        {
            "name": "Wojciech Nawalaniec",
            "email": "wnnawalaniec@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Timeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fullTest": [
            "phpunit",
            "infection"
        ]
    }
